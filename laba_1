для ( var i =  2 ; i < variant + 10 ; i ++ ) {
  для ( var z =  2 ; z < i; z ++ )
    если (i % z ==  0 )
      ToPrint =  false ;
  if (ToPrint ==  true )
    alert (i);
  ToPrint =  true ;
}
