alert ( « Задача № 3 » );

var variant =  prompt ( « Запишите свой вариант » );
var integer =  parseInt (вариант, 10 );
variant = integer;

если (вариант > = 7 )
  alert ( « Правильно » );
еще
  alert ( « Это неправильно » );

if (вариант % 2 == 0 ) {
  вариант + = 7 ;
  предупреждение (вариант);
  вариант - = 7 ;
}
else {
  вариант - = 3 ;
  предупреждение (вариант);
  вариант + = 3 ;
}

